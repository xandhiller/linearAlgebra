\section{Question 1}

\begin{equation}    
  \mathbf{A} = 
  \begin{bmatrix}
    3  & 0 & -1 & 0 \\
    9  & 1 & -3 & 4 \\
    -6 & 0 & -2 & 2 \\
    0  & 2 &  4 & 8 \\
  \end{bmatrix} 
\end{equation}

For LU-decomposition with Doolittle's method: 

\begin{equation}    
  \mathbf{A} = \mathbf{LU} = 
  \begin{bmatrix}
    1         & 0       & 0      & 0 \\
    \ l_{21}  & 1       & 0      & 0 \\
    \ l_{31}  & l_{32}  & 1      & 0 \\
    \ l_{41}  & l_{42}  & l_{43} & 1 \\
  \end{bmatrix} 
  \begin{bmatrix}
    u_{11} & u_{12} & u_{13} & u_{14} \\
    0      & u_{22} & u_{23} & u_{24} \\
    0      & 0      & u_{33} & u_{34} \\
    0      & 0      & 0      & u_{44} \\
  \end{bmatrix} 
\end{equation}

Hence:


\begin{equation}
  l_{21} = \frac{a_{21}}{u_{11}} = \frac{9}{3}
\end{equation}

\begin{equation}    
  u_{22} = 
  a_{22} - l_{21} u_{12} = 
  (1) - (3)(0) = 
  1
\end{equation}

\begin{equation}    
  u_{23} = 
  a_{23} - l_{21} u_{13} =
  (-3) - (3)(-1) =
  0
\end{equation}

\begin{equation}    
  u_{24} = 
  a_{24} - l_{21} u_{14} = 
  (4) - (3)(0) = 
  4
\end{equation}

\begin{equation}    
  l_{31} = 
  \frac{a_{31}}{u_{11}} = 
  \frac{-6}{3} = 
  -2
\end{equation}

\begin{equation}    
  l_{32} =
  \frac
  {a_{32} - l_{31} u_{12}}
  {u_{22}}
  =
  \frac{0 - (-2)(0)}{1} =
  0
\end{equation}

\begin{equation}    
  u_{33} = 
  a_{33}  - l_{31} u_{13} - l_{32} u_{23} =
  (-2) - (-2)(-1) - (0)(0) =
  -4
\end{equation}

\begin{equation}    
  u_{34} = 
  a_{34} - l_{31} u_{14} - l_{32} u_{24} =
  (2) - (-2)(0) - (0)(4) = 
  2
\end{equation}

\begin{equation}
  l_{41} = 
  \frac {a_{41}} {u_{11}} =
  \frac {0} {3} =
  0
\end{equation}

\begin{equation}    
  l_{42} =
  \frac {a_{42} - l_{41} u_{12}} {u_{22}} =
  \frac {(2) - (0)(0)} {(1)} = 
  2
\end{equation}

\begin{equation}    
  l_{43} = 
  \frac {a_{43} - l_{41} u_{13} - l_{42} u_{23}} {u_{33}} =
  \frac {(4) - (0)(-5) - (0)(0)} {-4} = 
  -1
\end{equation}

\begin{equation}    
  a_{44} - l_{41} u_{14} - l_{42} u_{24} - l_{43} u_{34} = 
  (8) - (0)(0) - (2)(4) -  (2)\bigg(\frac{1}{2}\bigg) =
  8 - 8 + 2 = 
  2
\end{equation}

Therefore:

\begin{equation}    
  \mathbf{A} = \mathbf{LU} =
  \begin{bmatrix}
    1 &  0 &  0 & 0 \\
    3 &  1 &  0 & 0 \\
    -2 &  0 &  1 & 0 \\
     0 &  2 & -1 & 1 \\
  \end{bmatrix} 
  \begin{bmatrix}
     3 &  0 & -1 & 0 \\
     0 &  1 &  0 & 4 \\
     0 &  0 & -4 & 2 \\
     0 &  0 &  0 & 2 \\
  \end{bmatrix}
\end{equation}



